{
  "author": "Aaron Reisman <aaron@lifeiscontent.net>",
  "dependencies": {
    "@fastify/auth": "4.4.0",
    "@fastify/autoload": "5.8.0",
    "@fastify/bearer-auth": "9.3.0",
    "@fastify/cors": "8.5.0",
    "@fastify/postgres": "5.2.2",
    "@fastify/swagger": "8.13.0",
    "@fastify/swagger-ui": "2.0.1",
    "@fastify/type-provider-typebox": "4.0.0",
    "@sinclair/typebox": "0.32.9",
    "ajv-errors": "3.0.0",
    "bcryptjs": "2.4.3",
    "fastify": "4.25.2",
    "fastify-list-routes": "1.0.0",
    "fastify-plugin": "4.5.1",
    "jsonwebtoken": "9.0.2",
    "kysely": "0.27.2",
    "pg": "8.11.3",
    "slugify": "1.6.6"
  },
  "description": "Example Realworld API using Fastify, Kysley, Postgres and TypeScript",
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "20.11.0",
    "@types/pg": "8.10.9",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "dotenv": "16.3.1",
    "eslint": "8.56.0",
    "kysely-codegen": "0.11.0",
    "prettier": "3.2.1",
    "tsx": "4.7.0",
    "type-fest": "4.9.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "20.x"
  },
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "name": "fastify-realworld",
  "packageManager": "pnpm@8.14.1+sha256.2df78e65d433d7693b9d3fbdaf431b2d96bb4f96a2ffecd51a50efe16e50a6a8",
  "scripts": {
    "dev": "tsx watch --import ./dotenv.js ./src/index.ts",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "kysley-codegen": "kysely-codegen --env-file .env.development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "tsx --import ./dotenv.js scripts/migrate.ts",
    "migrate:down": "tsx --import ./dotenv.js scripts/migrate/down.ts",
    "prepare": "tsc",
    "seed": "tsx --import ./dotenv.js scripts/seed.ts",
    "start": "node lib/src/index.js",
    "test": "tsx --import ./dotenv.js --test src/routes/api/articles.test.ts",
    "test:coverage": " pnpm test --experimental-test-coverage",
    "type:check": "tsc --noEmit"
  },
  "type": "module",
  "version": "1.0.0"
}
